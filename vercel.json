{
  "version": 2,
  "name": "SQLCoder-Force-Deploy",
  "outputDirectory": "new-public",
  "functions": {
    "api/server.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/new-public/$1"
    }
  ]
}